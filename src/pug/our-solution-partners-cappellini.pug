extends includes/_layout.pug


block prepend head
    - var pageClass = "page our-solution-partners-cappellini"
    - var fixedClass = " fixed "

block content
    .ems-flex
            +picture('ems-responsive', 'no', 'upload/home-page-slide/mnSlide1x1.jpg', 'upload/home-page-slide/mnSlide1x2.jpg', 'upload/home-page-slide/mnSlide1.jpg')
    .ems-container-lg
        .d-ems-grid.d-ems-grid-cover-top
            .ems-breadcrumb-absolute
                include includes/_breadcrumb.pug
    .d-ems-container-full
        div
            h1.page-title-white.d-line-height-37.d-ems-text-center.d-font-size-36.m-l-20.font-size-32.m-b-0 Cappellini
            div.mobile-renctangle.displaylg-none
            div.mobile-mini-renctangle.displaylg-none
    //OUR PARTNER CAPPELLİNİ GRID
    .ems-container-lg
        .ems-container-small.m-auto.p-l-20.p-r-20.p-lg-r-30.p-lg-l-30
            p.cappellini-text.m-lg-t-80.d-line-height-24.d-font-size-20.m-lg-b-0.m-t-30.m-b-0.line-height-1-4.font-size-20 ERSA is a reseller of Italian design firm Cappellini in Turkey since 2015.
            .m-lg-t-60.m-t-60.m-b-0.mobile-logo-size
                +picture('', 'no', 'upload/logo/cappellini.svg', 'upload/logo/cappellini.svg', 'upload/logo/cappellini.svg')
            .m-lg-t-20.m-t-20
                p.cappellini-desc.d-font-size-16.d-line-height-24.m-lg-b-0.font-size-16.line-height-24.m-b-0 Founded in 1946, Cappellini has evolved through the years into a sought-after brand of contemporary and avant-garde design by manufacturing innovative furniture of the highest quality. Cappellini, which manufactures modular furniture and provides solutions for residential and office spaces, is known for combining design and advanced technologies with traditional craftsmanship. Cappellini collections, characterized by experimentation and exploration of new ways of living, can furnish the whole domestic landscape from the living room to the studio…
                p.display-lg-none.color-red.font-size-15.m-t-42.m-b-0.ems-flex.ems-flex-middle View More
                    +icon('plus')
    .ems-container-lg
        div.m-lg-t-80.m-t-63
            p.d-font-size-17.m-lg-b-0.m-lg-t-0.p-l-20.p-r-20.p-lg-0.font-size-17.m-b-10 Products from <br> 
            p.d-font-size-32.font-weight-bold.m-lg-b-0.m-lg-t-10.p-l-20.p-r-20.p-lg-0.font-size-32.m-t-0  Cappellini
    .ems-container-lg.p-l-20
        .m-lg-t-40.m-t-40
            nav
                ul.d-ems-flex.ems-flex.auto-scroll.p-b-30
                    li.p-r-20.d-font-size-17.color-red.font-size-17
