extends includes/_layout.pug

block prepend head
    - var pageClass = "page our-stores"
    - var fixedClass = " fixed"

block content
    .ems-container-full
        .ems-breadcrumb-absolute
            include includes/_breadcrumb.pug
        .ems-flex
            +picture('ems-responsive', 'no', 'upload/home-page-slide/mnSlide1x1.jpg', 'upload/home-page-slide/mnSlide1x2.jpg', 'upload/home-page-slide/mnSlide1.jpg')
        .ems-container-lg
            .ems-prd-list.ems-prd-list-page.ems-section.m-t-30.m-lg-t-60
                ul.ems-flex.ems-flex-wrap.ems-flex-prd-list-wrapper
                    -for (var y = 1; y <= 9; y++)
                        li.ems-prd
                            .ems-prd-inner
                                +image('no', 'https://via.placeholder.com/800x670/DDDDDD/000000')
                                .ems-prd-title.font-size-20.font-weight-bold.m-t-10 Istanbul Office Store
                                .ems-prd-desc.font-size-16.m-t-10 Ord.Prof.Dr. Fahrettin Kerim Gökay Caddesi No:60 Altunizade Üsküdar 34660 Istanbul
                                .ems-prd-phone.font-size-16.m-t-20 P: +90 216 415 59 11
                                .ems-prd-fax.font-size-16.m-t-10 F: +90 216 415 56 75
                                .ems-prd-email.font-size-16.m-t-10 E: info@ersamobilya.com
                                .ems-prd-link.font-size-14.font-weight-bold.m-t-40 Get directions
                                +link('')(href="product-detail.html" class="ems-prd-link btn-full")
            .ems-flex.ems-flex-center
                .btn.btn-transparent.btn-icon.show-more
                    span Show more
                        +icon('plus m-l-5')